  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://effective-disco-xq46q7rqqp53pvpw-3000.app.github.dev/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
      "default_title": "Open Traversal Session",
      "default_icon": {
        "16": "gnost.jpg",
        "48": "gnost.jpg",
        "128": "gnost.jpg"
      }
    } ,
    "icons": {
    "16": "gnost.jpg",
    "48": "gnost.jpg",
    "128": "gnost.jpg"
  }
  
}
